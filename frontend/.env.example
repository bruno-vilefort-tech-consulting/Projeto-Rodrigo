# Backend URL
REACT_APP_BACKEND_URL=https://api.yourdomain.com

# System Name
REACT_APP_NAME_SYSTEM=ChatIA

# Company Name (legacy - use NAME_SYSTEM instead)
REACT_APP_COMPANY_NAME=Your Company Name

# Support Contact
REACT_APP_NUMBER_SUPPORT=+5511999999999

# Auto close tickets after X hours (default: 9999 = disabled)
REACT_APP_HOURS_CLOSE_TICKETS_AUTO=9999

# Theme Colors
REACT_APP_PRIMARY_COLOR=#6B46C1
REACT_APP_PRIMARY_DARK=#4C1D95

# Facebook Integration (optional)
REACT_APP_FACEBOOK_APP_ID=

# Business Management Required (TRUE/FALSE)
REACT_APP_REQUIRE_BUSINESS_MANAGEMENT=FALSE

# Environment
NODE_ENV=production
